                "numbers_manual": 8,
                "teachingbook": null,
                "referencebook": "<span class=\"px12\">大勢至菩薩念佛圓通章疏鈔研習講義 </span><a href=\"http://www.amtb.org.tw/pdf/05-06jy.pdf\"><img \r\n\r\nsrc=\"../image/pdf_logo.gif\" border=\"0\" align=\"absmiddle\"></a>",
